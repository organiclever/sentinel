{
  "name": "sentinel",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "dev:sentinel-ui": "npm run dev --prefix apps/sentinel-ui",
    "dev:sentinel-be": "cd apps/sentinel-be && ./mvnw spring-boot:run",
    "test:e2e": "npm run test --prefix apps/sentinel-e2e",
    "test:e2e:ci": "start-server-and-test 'npm run dev:sentinel-ui' http://localhost:3300 'npm run test:e2e'",
    "test:e2e:ci2": "start-server-and-test 'npm run dev:sentinel-ui' http://localhost:3300 'npm run dev:sentinel-be' http://localhost:8080/api/v1/hello 'npm run test:e2e'",
    "test:e2e:ui": "npm run test:ui --prefix apps/sentinel-e2e",
    "prepare": "npx husky install",
    "concurrently:dev-and-test": "concurrently \"npm run dev:sentinel-ui\" \"npm run test:e2e\"",
    "concurrently:install": "concurrently \"npm install --prefix apps/sentinel-ui\" \"npm install --prefix apps/sentinel-e2e\""
  },
  "author": "",
  "license": "ISC",
  "description": "",
  "devDependencies": {
    "concurrently": "^7.0.0",
    "husky": "^9.1.5",
    "prettier": "^3.3.3",
    "start-server-and-test": "^2.0.5",
    "wait-on": "^8.0.0"
  },
  "volta": {
    "node": "20.17.0",
    "npm": "10.8.3"
  }
}
